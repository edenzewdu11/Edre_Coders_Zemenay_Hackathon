{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "tsconfig.build.json",
    "webpackConfigPath": "webpack.config.js"
  },
  "generateOptions": {
    "spec": {
      "class": false,
      "component": false,
      "service": false,
      "module": false,
      "controller": false,
      "guard": false,
      "interceptor": false,
      "pipe": false,
      "decorator": false,
      "filter": false
    }
  },
  "projects": {
    "zemenay-backend": {
      "type": "application",
      "root": "",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.app.json"
      }
    }
  }
}
